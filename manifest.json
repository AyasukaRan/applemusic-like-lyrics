{
    "manifest_version": 1,
    "name": "Apple Music-like lyrics",
    "author": "SteveXMH",
    "author_links": ["https://github.com/Steve-xmh", "https://github.com/Steve-xmh/applemusic-like-lyrics"],
    "description": "一个基于 BetterNCM 的类 Apple Music 歌词显示插件",
    "preview": "preview.svg",
    "version": "1.1.0",
    "type": "extension",
    "requirements": [],
    "incompatible": ["RefinedNowPlaying"],
    "betterncm_version": ">=1.0.0",
    "injects": {
        "Main": [
            {
                "file": "index.js"
            }
        ]
    },
    "hijacks": {
        ">= 2.10.4 <= 2.10.6": {
            "orpheus://orpheus/pub/core.": {
                "type": "replace",
                "from": "=Function.prototype;",
                "to": "=FakeFunction;"
            }
        }
    }
}