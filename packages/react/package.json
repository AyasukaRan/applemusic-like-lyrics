{
  "name": "@applemusic-like-lyrics/react",
  "packageManager": "yarn@3.6.1",
  "version": "0.0.13",
  "description": "AMLL 组件库的 React 绑定",
  "repository": {
    "url": "https://github.com/Steve-xmh/applemusic-like-lyrics.git",
    "directory": "packages/react",
    "type": "git"
  },
  "license": "GPL-3.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "build": "vite build",
    "build:dev": "vite build",
    "fmt": "rome format --write ./src",
    "dev": "vite dev"
  },
  "main": "./dist/amll-react.cjs",
  "module": "./dist/amll-react.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/amll-react.mjs",
      "require": "./dist/amll-react.cjs"
    }
  },
  "devDependencies": {
    "@pixi/app": "^7.2.4",
    "@pixi/core": "^7.2.4",
    "@pixi/display": "^7.2.4",
    "@pixi/filter-blur": "^7.2.4",
    "@pixi/filter-color-matrix": "^7.2.4",
    "@pixi/sprite": "^7.2.4",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "^12.1.3",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {
    "@applemusic-like-lyrics/core": "workspace:^"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
